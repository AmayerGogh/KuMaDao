    <script src="/Assets/js/IsometricGrids/modernizr.custom.js"></script>
    <script src="/Assets/js/IsometricGrids/imagesloaded.pkgd.min.js"></script>
    <script src="/Assets/js/IsometricGrids/masonry.pkgd.min.js"></script>
    <script src="/Assets/js/IsometricGrids/dynamics.min.js"></script>
    <script src="/Assets/js/IsometricGrids/main.js"></script>
    <script src="~/Assets/js/IsometricGrids/classie.js"></script>
    <script>
		(function() {
			function getRandomInt(min, max) {
				return Math.floor(Math.random() * (max - min + 1)) + min;
			}

			new IsoGrid(document.querySelector('.isolayer--deco1'), {
				transform : 'translateX(33vw) translateY(-340px) rotateX(45deg) rotateZ(45deg)',
				stackItemsAnimation : {
					properties : function(pos) {
						return {
							translateZ: (pos+1) * 30,
							rotateZ: getRandomInt(-4, 4)
						};
					},
					options : function(pos, itemstotal) {
						return {
							type: dynamics.bezier,
							duration: 500,
							points: [{"x":0,"y":0,"cp":[{"x":0.2,"y":1}]},{"x":1,"y":1,"cp":[{"x":0.3,"y":1}]}],
							delay: (itemstotal-pos-1)*40
						};
					}
				}
			});
		})();
    </script>
    <script src="/Assets/js/threeDbg/jquery.logosDistort.min.js"></script>
    <script src="/Assets/js/threeDbg/jquery.particleground.min.js"></script>
    <script>
	    var particles = true,
	        particleDensity,
	        options = {
	            effectWeight: 1,
	            outerBuffer: 1.08,
	            elementDepth: 220
	        };

	    $(document).ready(function() {

	        $("#demo1").logosDistort(options);
	        if (particles) {
	            particleDensity = window.outerWidth * 7.5;
	            if (particleDensity < 13000) {
	                particleDensity = 13000;
	            } else if (particleDensity > 20000) {
	                particleDensity = 20000;
	            }
	            return $('#particle-target').particleground({
	                dotColor: '#009688',
	                lineColor: '#009688',
	                density: particleDensity.toFixed(0),
	                parallax: false
	            });
	        }
	    });
    </script>
